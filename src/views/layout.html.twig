<!DOCTYPE html>
<html lang="en" ng-app="myApp">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Home</title>
    <link href="/css/app.css" rel="stylesheet" type="text/css">
    <script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.2.16/angular.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.2.16/angular-route.min.js"></script>
    <script type="text/javascript">
        if (typeof angular == 'undefined') {
            document.write(unescape('%3Cscript src="/js/libs/angular/angular.min.js" type="text/javascript"%3E%3C/script%3E'));
            document.write(unescape('%3Cscript src="/js/libs/angular/angular-route.min.js" type="text/javascript"%3E%3C/script%3E'));
        }
    </script>
    <script src="/js/app.js"></script>
</head>
<body>
    <div class="container">
        {% block content %}{% endblock %}
    </div>
</body>
</html>
